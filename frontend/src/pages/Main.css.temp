.event-title-main {
  font-size: 15px;
  font-weight: 700;
  color: var(--color-text-white);
  margin-bottom: 8px;
  text-transform: uppercase;
  letter-spacing: 0.5px;
  word-break: break-word;
  max-width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
